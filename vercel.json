{
  "functions": {
    "api/main.py": { "runtime": "python3.12", "maxDuration": 30, "memory": 512 }
  },
  "builds": [
    { "src": "api/main.py", "use": "@vercel/python" },
    { "src": "dashboards/nextjs-dashboard/package.json", "use": "@vercel/next" }
  ],
  "routes": [
    { "src": "/predict", "dest": "api/main.py" }
  ]
}
